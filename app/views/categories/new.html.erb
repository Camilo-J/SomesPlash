<h1>New Category</h1>
<%= form_with model: @category do |form| %>
  <div>
    <% @category.errors.full_messages.each do |message|%>
      <%= message%>
    <%end%>
  </div>
  <div>
    <%= form.label :name%></br>
    <%= form.text_field :name%>
  </div>
  <div>
    <%= form.label :description%></br>
    <%= form.text_area :description%>
  </div>  
  <%= form.submit %>
<% end %>
